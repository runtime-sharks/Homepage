<!--Tracking-->
<script>
	window.ga = window.ga || function() { (ga.q = ga.q || []).push(arguments) }
	ga.l =+ new Date
	ga('create', '#(trackingId)', { 'storage': 'none', 'storeGac': false })
	ga('set', 'anonymizeIp', true);
	ga('send', 'pageview')
</script>
